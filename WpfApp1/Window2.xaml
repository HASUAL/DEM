<Window x:Class="WpfApp1.Window2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="Список агентов" Height="450" Width="810" Background="#FFFDE5E5">
    <Grid>
        <TextBlock Text="Список агентов" Height="25" Margin="59,22,623,372"/>
        <ComboBox x:Name="peopleComboBox" Height="30" Width="450" HorizontalAlignment="Center" VerticalAlignment="Top" Background="#FFFFAAAA" MouseDoubleClick="ClickAgent" Margin="59,52,293,0" RenderTransformOrigin="0.522,0.571" SelectionChanged="peopleComboBox_SelectionChanged">
            <TextBlock>Первый агент</TextBlock>
        </ComboBox>
        <TextBlock Text="Фильтр" Height="25" Margin="552,22,130,372"/>
        <ComboBox x:Name="peopleComboBoxSort" Height="30" Width="150" HorizontalAlignment="Center" VerticalAlignment="Top" Background="#FFFFAAAA" Margin="552,52,100,0">
            <TextBlock>Наименование</TextBlock>
            <TextBlock>Размер скидки</TextBlock>
            <TextBlock>Приоритет</TextBlock>
        </ComboBox>

    </Grid>
</Window>
